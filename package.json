{
  "name": "kaner",
  "version": "1.0.0",
  "description": "txt reader",
  "main": "main.js",
  "scripts": {
    "start": "/Applications/Electron.app/Contents/MacOS/Electron .",
    "package": "electron-packager ./ --all --version=1.4.3 --icon=./ico/ico.ico --out=../exec/ --overwrite",
    "package-win64": "electron-packager ./ --platform=win32 --arch=x64 --version=1.4.3 --icon=./ico/ico.ico --out=../exec/ --overwrite",
    "package-win32": "electron-packager ./ --platform=win32 --arch=ia32 --version=1.4.3 --icon=./ico/ico.ico --out=../exec/ --overwrite",
    "package-osx": "electron-packager ./ --platform=darwin --arch=x64 --version=1.4.3 --icon=./ico/ico.ico --out=../exec/ --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanrin/Kaner.git"
  },
  "keywords": [
    "reader",
    "txt"
  ],
  "author": "Orz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kanrin/Kaner/issues"
  },
  "homepage": "https://github.com/kanrin/Kaner#readme",
  "dependencies": {
    "fs": "0.0.1-security",
    "iconv-lite": "^0.4.13",
    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "electron-packager": "^7.3.0"
  }
}
